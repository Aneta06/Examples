public class Recursion2 {
    public static void main(String[] args) {

        int n = m1(7);
        System.out.println(n);

    }

    public static int m1(int n) {
        System.out.println(n);
        if (n == 1) {
            return 25;
        } else {
            return m1(n - 1);

        }
    }
}
