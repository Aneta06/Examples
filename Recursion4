public class Recursion4 {

    public static void main (String[] args){

       int n = m4(1);
        System.out.println(n);
    }

    public static int m4(int n){
        System.out.println(n);
        if (n == 6){
            return n;
        } else {
            return n + m4(n+1);
        }
    }

}
